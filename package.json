{
  "name": "ream",
  "version": "1.0.0-beta.2",
  "description": "unopinionated, universal Vue.js app made simple.",
  "repository": {
    "url": "ream/ream",
    "type": "git"
  },
  "main": "lib/index.js",
  "bin": "bin/cli.js",
  "files": [
    "lib",
    "app",
    "bin",
    "plugins"
  ],
  "publishConfig": {
    "tag": "beta"
  },
  "scripts": {
    "test:cov": "BABEL_ENV=test jest --coverage && npm run lint",
    "test": "BABEL_ENV=test jest && npm run lint",
    "lint": "xo",
    "precommit": "npm t",
    "postinstall": "./node_modules/.bin/opencollective-postinstall || exit 0"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "eslint-config-rem": "^3.0.0",
    "husky": "^0.13.2",
    "jest-cli": "^19.0.2",
    "opencollective-postinstall": "^1.0.13",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem",
    "esnext": true,
    "envs": [
      "jest"
    ],
    "globals": [
      "jasmine"
    ],
    "ignores": [
      "examples/**/*",
      "app/**/*",
      "**/dist/**",
      "**/fixture/**/*"
    ],
    "rules": {
      "guard-for-in": 0,
      "import/no-dynamic-require": 0,
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-load-config": "^0.1.0",
    "babel-loader": "^6.4.1",
    "babel-preset-vue-app": "^0.5.1",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "co": "^4.6.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "es6-promise": "^4.1.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "fs-promise": "^2.0.0",
    "get-port": "^3.0.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "internal-ip": "^1.2.0",
    "lru-cache": "^4.0.2",
    "memory-fs": "^0.4.1",
    "path-match": "^1.2.4",
    "path-to-regexp": "^1.7.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.16.0",
    "tildify": "^1.2.0",
    "update-notifier": "^2.1.0",
    "vue": "^2.2.5",
    "vue-loader": "^11.3.2",
    "vue-meta": "^0.5.5",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.5",
    "vue-ssr-webpack-plugin": "^1.0.2",
    "vue-template-compiler": "^2.2.5",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2",
    "webpack": "^2.3.1",
    "webpack-chain": "^3.0.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-node-externals": "^1.5.4",
    "yargs": "^7.0.2"
  },
  "peerDependencies": {},
  "babel": {
    "babelrc": false,
    "env": {
      "test": {
        "presets": [
          "vue-app"
        ]
      }
    }
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ream",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
